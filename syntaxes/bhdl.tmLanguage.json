{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "bhdl",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.bhdl",
				"match": "\\b(circuit|part|pin|layout|canvas|param|wire|net|series|parallel|bus|hstack|vstack|rotate|at)\\b"
			}]
		}
	},
	"scopeName": "source.bhdl",
	"injectionSelector": "L:source.racket"
}